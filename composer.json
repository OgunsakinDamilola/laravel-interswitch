{
    "name": "ogunsakindamilola/laravel-interswitch",
    "description": "A laravel package for Interswitch",
    "license": "MIT",
    "authors": [
        {
            "name": "Ogunsakin Damilola",
            "email": "ogunsakin191@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "OgunsakinDamilola\\Interswitch\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OgunsakinDamilola\\Interswitch\\InterswitchServiceProvider"
            ],
            "aliases": {
                "Interswitch": "OgunsakinDamilola\\Interswitch\\Facade\\Interswitch"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "dev-master@dev",
        "orchestra/testbench": "5.3.0"
    }
}
